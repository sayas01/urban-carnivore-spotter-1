import React, { Component } from 'react';
import { withRouter } from 'react-router-dom';

import { Map } from 'immutable';
import axios from "axios";

const speciesImageMap = Map({'blackbear':'../assets/blackbear.png'});

class ResourceCard extends Component {
    state = {
        resourceUrl: null,
    };

    componentDidMount() {
        const { match: { params: { id } } } = this.props;
        console.log(id);
        this.setState({resourceUrl: speciesImageMap.get(id.toLowerCase())})
    }

    render() {
        console.log(speciesImageMap.get(id.toLowerCase()));
        return (
            <img src={ require('../assets/speciesCard/blackbear.png') } />
        );
    }
}

export default ResourceCard;
